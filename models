from django.db import models

class Bookings(models.Model):
    number = models.TextField()                       #field_name = models.CharField(max_length= , help_text= , choices= , Null etc)
    cntr_number = models.TextField()
    eta_date = models.DateField()

class Meta:
    ordering = ['eta_date']

    def __str__(self):
        return self.text[:10]

